{
  "name": "gg.sf.ja",
  "version": "1.0.0",
  "description": "GoGoJungle VN FX project",
  "author": "Nguyen Trong Hieu <nguyentronghieu@gogojungle.vn>",
  "private": true,
  "main": "index.js",
  "engines": {
    "node": "10.16.0"
  },
  "scripts": {
    "build:staging": "$NVM_DIR/versions/node/v$npm_package_engines_node/lib/node_modules/yarn/bin/yarn build:lang:staging && concurrently \"$NVM_DIR/versions/node/v$npm_package_engines_node/lib/node_modules/yarn/bin/yarn build:staging:desktop\" \"$NVM_DIR/versions/node/v$npm_package_engines_node/lib/node_modules/yarn/bin/yarn build:staging:mobile\"",
    "build:staging:desktop": "nuxt build --config-file desktop/nuxt.config.js",
    "build:staging:mobile": "nuxt build --config-file mobile/nuxt.config.js",
    "build:prod": "$NVM_DIR/versions/node/v$npm_package_engines_node/lib/node_modules/yarn/bin/yarn build:lang:prod && concurrently \"$NVM_DIR/versions/node/v$npm_package_engines_node/lib/node_modules/yarn/bin/yarn build:prod:desktop\" \"$NVM_DIR/versions/node/v$npm_package_engines_node/lib/node_modules/yarn/bin/yarn build:prod:mobile\"",
    "build:prod:desktop": "nuxt build --config-file desktop/nuxt.config.js",
    "build:prod:mobile": "nuxt build --config-file mobile/nuxt.config.js",
    "build:lang:win": "%NVM_HOME%/v%npm_package_engines_node%/node ../bin/google-sheet.js",
    "build:lang:staging": "$NVM_DIR/versions/node/v$npm_package_engines_node/bin/node ../bin/google-sheet.js",
    "build:lang:prod": "$NVM_DIR/versions/node/v$npm_package_engines_node/bin/node ../bin/google-sheet.js prod",
    "build:analyze:desktop": "ANALYZE=true nuxt build --config-file desktop/nuxt.config.js",
    "build:analyze:mobile": "ANALYZE=true nuxt build --config-file mobile/nuxt.config.js",
    "build:analyze": "concurrently \"%NVM_HOME%/v%npm_package_engines_node%/yarn build:analyze:desktop\" \"%NVM_HOME%/v%npm_package_engines_node%/yarn build:analyze:mobile\"",
    "prestart": "cd .. && %NVM_HOME%/v%npm_package_engines_node%/yarn && cd ./ja && %NVM_HOME%/v%npm_package_engines_node%/yarn",
    "start": "%NVM_HOME%/v%npm_package_engines_node%/yarn build:lang:win && %NVM_HOME%/v%npm_package_engines_node%/node --max-old-space-size=4096 .",
    "prepm2": "cd .. && $NVM_DIR/versions/node/v$npm_package_engines_node/lib/node_modules/yarn/bin/yarn && cd ./ja && $NVM_DIR/versions/node/v$npm_package_engines_node/lib/node_modules/yarn/bin/yarn",
    "pm2": "pm2 start ecosystem.config.js",
    "styleguide": "concurrently \"npm run styleguide:desktop\"",
    "styleguide:mobile": "vue-styleguidist --config ./styleguide.mobile.config.js server",
    "styleguide:desktop": "vue-styleguidist --config ./styleguide.desktop.config.js server",
    "styleguide:build:desktop": "vue-styleguidist --config ./styleguide.desktop.config.js build",
    "styleguide:build:mobile": "vue-styleguidist --config ./styleguide.mobile.config.js build",
    "styleguide:run": "concurrently \"npm run styleguide:build:desktop\" \"npm run styleguide:build:mobile\" \"pm2 start styleguide.eco.config.js\"",
    "lint": "eslint --ext .js,.vue --ignore-path ../.gitignore --fix .",
    "aws": "node .",
    "aws:build": "node ../bin/google-sheet.js && concurrently \"nuxt build --config-file desktop/nuxt.config.js\" \"nuxt build --config-file mobile/nuxt.config.js\""
  },
  "dependencies": {
    "@fullcalendar/core": "^4.1.0",
    "@fullcalendar/daygrid": "^4.1.0",
    "@ggj/utils": "1.0.9",
    "axios": "^0.18.0",
    "body-parser": "^1.18.3",
    "bootstrap": "3.3.7",
    "concurrently": "^4.0.1",
    "glob-all": "^3.1.0",
    "highcharts": "^6.1.1",
    "highcharts-custom-events": "^3.0.3",
    "highcharts-v9": "npm:highcharts@9.1.2",
    "ip": "^1.1.5",
    "is-retry-allowed": "^1.1.0",
    "jquery": "^3.2.1",
    "jquery.dotdotdot": "^3.2.3",
    "nuxt": "^1.4.2",
    "owl.carousel": "^2.3.4",
    "purgecss-webpack-plugin": "^1.5.0",
    "slack-node": "^0.1.8",
    "vee-validate": "2.0.9",
    "vue": "^2.5.17",
    "vue-i18n": "^8.0.0",
    "vue-tel-input": "2.0.17",
    "vue-template-compiler": "^2.5.17",
    "vue-the-mask": "^0.11.1",
    "vue-webpack-loaders": "^1.0.8"
  },
  "devDependencies": {
    "eslint-loader": "^2.1.0",
    "http-proxy-middleware": "^0.19.0",
    "less": "^4.1.2",
    "less-loader": "^4.1.0",
    "node-sass": "^4.12.0",
    "sass-loader": "^7.1.0",
    "vue-styleguidist": "^3.15.2"
  }
}
